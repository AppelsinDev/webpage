---
const { heading, subheading } = Astro.props;
---

<div class="inline-flex max-w-5xl flex-col items-center justify-start gap-4">
  <div
    class="self-stretch text-center font-inter text-5xl font-bold text-white"
  >
    {heading}
  </div>
  <div
    class="self-stretch text-center font-inter text-xl font-normal text-zinc-400"
  >
    {subheading}
  </div>
</div>
